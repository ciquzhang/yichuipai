<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data></data>

    <com.zhy.autolayout.AutoLinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#f4f4f4">
        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/frag_my_login"
            android:layout_width="match_parent"
            android:layout_height="176px"
            android:background="#00b589">
            <com.baichao.yichuipai.view.RoundImageView
                android:id="@+id/frag_my_img"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="66px"
                android:background="@drawable/touxiang"
                android:src="@drawable/user_icon"/>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_toRightOf="@+id/frag_my_img"
                android:layout_marginLeft="50px"
                android:gravity="center_vertical">
                <TextView
                    android:id="@+id/frag_my_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="未登录"
                    android:textSize="36px"
                    android:textColor="#ffffff"/>

                <TextView
                    android:id="@+id/frag_my_msg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="个人信息"
                    android:textSize="28px"
                    android:layout_marginTop="20px"
                    android:textColor="#CCF0E7"/>
            </com.zhy.autolayout.AutoLinearLayout>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/icon_goin_w"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="20px"/>
        </com.zhy.autolayout.AutoRelativeLayout>

        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/frag_my_attention"
            android:layout_width="match_parent"
            android:layout_height="88px"
            android:background="#ffffff"
            android:layout_marginTop="20px">
            <ImageView
                android:id="@+id/frag_my_icon_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_my_attention"
                android:layout_marginLeft="18px"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/frag_my_icon_1"
                android:layout_centerVertical="true"
                android:textSize="32px"
                android:text="关注"
                android:layout_marginLeft="22px"/>

            <ImageView
                android:id="@+id/frag_my_img_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="20px"
                android:src="@mipmap/icon_goin_g"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/frag_my_img_1"
                android:layout_marginRight="12px"
                android:textColor="#999999"
                android:textSize="28px"/>
        </com.zhy.autolayout.AutoRelativeLayout>

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="178px"
            android:background="#ffffff"
            android:layout_marginTop="20px"
            android:orientation="vertical">
            <com.zhy.autolayout.AutoRelativeLayout
                android:id="@+id/frag_my_approve_person"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1">
                <ImageView
                    android:id="@+id/frag_my_icon_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/icon_my_personal_certificate"
                    android:layout_marginLeft="18px"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/frag_my_icon_2"
                    android:layout_centerVertical="true"
                    android:textSize="32px"
                    android:text="实名认证"
                    android:layout_marginLeft="22px"/>

                <TextView
                    android:id="@+id/frag_my_tv_approve_person"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/frag_my_img_2"
                    android:layout_marginRight="12px"
                    android:text="未认证"
                    android:textColor="#999999"
                    android:textSize="28px"/>

                <ImageView
                    android:id="@+id/frag_my_img_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20px"
                    android:src="@mipmap/icon_goin_g"/>
            </com.zhy.autolayout.AutoRelativeLayout>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="2px"
                android:background="#eeeeee"
                android:layout_marginLeft="20px"/>

            <com.zhy.autolayout.AutoRelativeLayout
                android:id="@+id/frag_my_approve_company"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1">
                <ImageView
                    android:id="@+id/frag_my_icon_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/icon_my_enterprise_certification"
                    android:layout_marginLeft="18px"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/frag_my_icon_3"
                    android:layout_centerVertical="true"
                    android:textSize="32px"
                    android:text="企业认证"
                    android:layout_marginLeft="22px"/>

                <TextView
                    android:id="@+id/frag_my_tv_approve_company"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/frag_my_img_3"
                    android:layout_marginRight="12px"
                    android:text="未认证"
                    android:textColor="#999999"
                    android:textSize="28px"/>

                <ImageView
                    android:id="@+id/frag_my_img_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20px"
                    android:src="@mipmap/icon_goin_g"/>
            </com.zhy.autolayout.AutoRelativeLayout>
        </com.zhy.autolayout.AutoLinearLayout>

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="178px"
            android:background="#ffffff"
            android:layout_marginTop="20px"
            android:orientation="vertical">
            <com.zhy.autolayout.AutoRelativeLayout
                android:id="@+id/frag_my_wallet"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1">
                <ImageView
                    android:id="@+id/frag_my_icon_4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/icon_my_wallet"
                    android:layout_marginLeft="18px"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/frag_my_icon_4"
                    android:layout_centerVertical="true"
                    android:textSize="32px"
                    android:text="钱包"
                    android:layout_marginLeft="22px"/>

                <ImageView
                    android:id="@+id/frag_my_img_4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20px"
                    android:src="@mipmap/icon_goin_g"/>
            </com.zhy.autolayout.AutoRelativeLayout>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="2px"
                android:background="#eeeeee"
                android:layout_marginLeft="20px"/>

            <com.zhy.autolayout.AutoRelativeLayout
                android:id="@+id/frag_my_safe"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1">
                <ImageView
                    android:id="@+id/frag_my_icon_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/icon_my_safety"
                    android:layout_marginLeft="18px"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/frag_my_icon_5"
                    android:layout_centerVertical="true"
                    android:textSize="32px"
                    android:text="安全性"
                    android:layout_marginLeft="22px"/>

                <ImageView
                    android:id="@+id/frag_my_img_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20px"
                    android:src="@mipmap/icon_goin_g"/>
            </com.zhy.autolayout.AutoRelativeLayout>
        </com.zhy.autolayout.AutoLinearLayout>

        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/frag_my_setting"
            android:layout_width="match_parent"
            android:layout_height="88px"
            android:background="#ffffff"
            android:layout_marginTop="20px">
            <ImageView
                android:id="@+id/frag_my_icon_6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_my_set"
                android:layout_marginLeft="18px"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/frag_my_icon_6"
                android:layout_centerVertical="true"
                android:textSize="32px"
                android:text="设置"
                android:layout_marginLeft="22px"/>

            <ImageView
                android:id="@+id/frag_my_img_6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="20px"
                android:src="@mipmap/icon_goin_g"/>

        </com.zhy.autolayout.AutoRelativeLayout>
    </com.zhy.autolayout.AutoLinearLayout>
</layout>
